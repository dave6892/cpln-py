        "memory": "128Mi",
        "cpu": "50m",
        "env": [],
        "inheritEnv": true,
        "lifecycle": {
          "postStart": {
            "exec": {
              "command": [
                "echo 'postStart'"
              ]
            }
          },
          "preStop": {
            "exec": {
              "command": [
                "echo 'preStop'"
              ]
            }
          }
        }
      }
    ],
    "firewallConfig": {
      "external": {
        "inboundAllowCIDR": [],
