    "containers": [
      {
        "name": "broker-api",
        "image": "/org/ledgestone/image/apalis-broker-dashboard-api:011cd2e-develop",
        "workingDir": "/",
        "ports": [
          {
            "protocol": "http",
            "number": 8080
